<h2>登录</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :用户名 %><br />
    <%= f.text_field :userid, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :密码 %><br />
    <%= f.password_field :password, autocomplete: "current-password" %>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>

  <div class="actions">
    <%= f.submit "登录" %>
  </div>
<% end %>




<p>
  <strong>忘记密码请联系管理员</strong>
</p>
