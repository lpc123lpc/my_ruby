<p id="notice"><%= notice %></p>

<h1>Historyborrowtables</h1>

<table>
  <thead>
    <tr>
      <th>Bookid</th>
      <th>Bookname</th>
      <th>Borrowerid</th>
      <th>Borrowdate</th>
      <th>Returndate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @historyborrowtables.each do |historyborrowtable| %>
      <tr>
        <td><%= historyborrowtable.bookid %></td>
        <td><%= historyborrowtable.bookname %></td>
        <td><%= historyborrowtable.borrowerid %></td>
        <td><%= historyborrowtable.borrowdate %></td>
        <td><%= historyborrowtable.returndate %></td>
        <td><%= link_to 'Show', historyborrowtable %></td>
        <td><%= link_to 'Edit', edit_historyborrowtable_path(historyborrowtable) %></td>
        <td><%= link_to 'Destroy', historyborrowtable, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

