<p id="notice"><%= notice %></p>

<h1>Bookcases</h1>

<table>
  <thead>
    <tr>
      <th>Bookcaseid</th>
      <th>Floor</th>
      <th>Partition</th>
      <th>Number</th>
      <th>Line</th>
      <th>Column</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookcases.each do |bookcase| %>
      <tr>
        <td><%= bookcase.bookcaseid %></td>
        <td><%= bookcase.floor %></td>
        <td><%= bookcase.partition %></td>
        <td><%= bookcase.number %></td>
        <td><%= bookcase.line %></td>
        <td><%= bookcase.column %></td>
        <td><%= link_to 'Show', bookcase %></td>
        <td><%= link_to 'Edit', edit_bookcase_path(bookcase) %></td>
        <td><%= link_to 'Destroy', bookcase, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bookcase', new_bookcase_path %>
